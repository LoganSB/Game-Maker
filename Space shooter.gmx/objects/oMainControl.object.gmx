if(keyboard_check_pressed(vk_escape))
{
    switch(curRoom)
    {
        case rTitleScreen:
             room_goto(rGameRoom);
             curRoom = room;
             break;
        case rGameRoom:
             if(paused)
             {
                instance_activate_all();
                paused = false;         
             }
             else
             {
                instance_deactivate_all(true);
                paused = true;
             }
             //room_goto(rPauseScreen);
             //curRoom = room;
             break;
        case rPauseScreen:
             room_goto(rGameRoom);

             curRoom = room;
             break;
    }
}

if(keyboard_check_pressed(ord('E')))
{
    switch(curRoom)
    {
        case rTitleScreen:
             instance_create(100, 100, oEnemy);
             break;
        case rGameRoom:
             instance_create(random(2000), random(2000), oEnemy);
             break;
        case rPauseScreen:
             game_end();
             break;
    }

}

